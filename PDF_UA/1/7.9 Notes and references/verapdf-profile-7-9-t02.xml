<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<profile xmlns="http://www.verapdf.org/ValidationProfile" flavour="PDFUA_1">
    <details creator="veraPDF Consortium" created="2020-03-13T10:58:04.098+03:00">
        <name>ISO 14289-1:2014 - 7.9 Notes and references - Unique id</name>
        <description>Each Note tag shall have unique ID key</description>
    </details>
    <hash></hash>
    <rules>
        <rule object="SENote" tags="major,machine,structure,note">
            <id specification="ISO_14289_1" clause="7.9" testNumber="2"/>
            <description>Each Note tag shall have unique ID key</description>
            <test>hasDuplicateNoteID == false</test>
            <error>
                <message>ID key %1 of the Note tag is non-unique</message>
                <arguments>
                    <argument>noteID</argument>
                </arguments>
            </error>
            <references/>
        </rule>
    </rules>
    <variables/>
</profile>
